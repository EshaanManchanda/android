<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".use_Q1">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="363dp"
                android:layout_height="82dp"
                android:layout_margin="20dp"
                android:layout_marginTop="36dp"
                android:fontFamily="@font/akaya_telivigala"
                android:text="@string/use_q1_1"
                android:textAlignment="center"
                android:textColor="@color/primary"
                android:textSize="34sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/alike"
                    android:text="@string/m"
                    android:textColor="@color/primary"
                    android:textSize="20sp" />

                <EditText
                    android:id="@+id/etnum1"
                    android:layout_width="wrap_content"
                    android:layout_height="48dp"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:ems="10"
                    android:fontFamily="@font/alike"
                    android:hint="@string/EM"
                    android:importantForAutofill="no"
                    android:inputType="number"
                    android:textColor="@color/primary"
                    android:textColorHint="#656565"
                    android:textSize="20sp"
                    tools:ignore="SpeakableTextPresentCheck,TextContrastCheck,TextContrastCheck,TextContrastCheck,DuplicateSpeakableTextCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="2"
                    android:fontFamily="@font/alike"
                    android:text="@string/e"
                    android:textColor="@color/primary"
                    android:textSize="20sp" />

                <EditText
                    android:id="@+id/etnum2"
                    android:layout_width="wrap_content"
                    android:layout_height="48dp"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:ems="10"
                    android:fontFamily="@font/alike"
                    android:hint="@string/EM"
                    android:importantForAutofill="no"
                    android:inputType="number"
                    android:textColor="@color/primary"
                    android:textColorHint="#656565"
                    android:textSize="20sp"
                    tools:ignore="SpeakableTextPresentCheck,TextContrastCheck,TextContrastCheck,TextContrastCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/alike"
                    android:text="@string/s"
                    android:textColor="@color/primary"
                    android:textSize="20sp" />

                <EditText
                    android:id="@+id/etnum3"
                    android:layout_width="wrap_content"
                    android:layout_height="48dp"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:ems="10"
                    android:fontFamily="@font/alike"
                    android:hint="@string/EM"
                    android:importantForAutofill="no"
                    android:inputType="number"
                    android:textColor="@color/primary"
                    android:textColorHint="#656565"
                    android:textSize="20sp"
                    tools:ignore="SpeakableTextPresentCheck,TextContrastCheck,TextContrastCheck,TextContrastCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/alike"
                    android:text="@string/h"
                    android:textColor="@color/primary"
                    android:textSize="20sp" />

                <EditText
                    android:id="@+id/ethindi"
                    android:layout_width="wrap_content"
                    android:layout_height="48dp"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:ems="10"
                    android:fontFamily="@font/alike"
                    android:hint="@string/EM"
                    android:importantForAutofill="no"
                    android:inputType="number"
                    android:textColor="@color/primary"
                    android:textColorHint="#656565"
                    android:textSize="20sp"
                    tools:ignore="SpeakableTextPresentCheck,TextContrastCheck,TextContrastCheck,TextContrastCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/alike"
                    android:text="@string/it"
                    android:textColor="@color/primary"
                    android:textSize="20sp" />

                <EditText
                    android:id="@+id/etit"
                    android:layout_width="wrap_content"
                    android:layout_height="48dp"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:ems="10"
                    android:fontFamily="@font/alike"
                    android:hint="@string/EM"
                    android:importantForAutofill="no"
                    android:inputType="number"
                    android:textColor="@color/primary"
                    android:textColorHint="#656565"
                    android:textSize="20sp"
                    tools:ignore="SpeakableTextPresentCheck,TextContrastCheck,TextContrastCheck,TextContrastCheck" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/btcheck"
        android:layout_width="206dp"
        android:layout_height="52dp"
        android:layout_marginTop="500dp"
        android:background="@drawable/shapes"
        android:backgroundTint="@color/primary"
        android:backgroundTintMode="add"
        android:text="@string/check"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>