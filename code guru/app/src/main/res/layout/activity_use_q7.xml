<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffff3"
    tools:context=".use_Q7">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="363dp"
        android:layout_height="82dp"
        android:layout_margin="20dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/akaya_telivigala"
        android:text="@string/javaqq6"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="316dp"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:fontFamily="@font/alike"
                android:text="Enter Number"
                android:textColor="#000000"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/etnum1"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:ems="10"
                android:fontFamily="@font/alike"
                android:hint="@string/enter_here"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#000000"
                android:textColorHint="#656565"
                android:textSize="20sp"
                tools:ignore="SpeakableTextPresentCheck,TextContrastCheck,TextContrastCheck,TextContrastCheck,DuplicateSpeakableTextCheck" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="447dp"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_marginLeft="10dp"
                android:layout_weight="2"
                android:fontFamily="@font/alike"
                android:text="Enter Power"
                android:textColor="#000000"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/etnum2"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:ems="10"
                android:fontFamily="@font/alike"
                android:hint="@string/enter_here"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#000000"
                android:textColorHint="#656565"
                android:textSize="20sp"
                tools:ignore="SpeakableTextPresentCheck,TextContrastCheck,TextContrastCheck,TextContrastCheck" />
        </LinearLayout>

        <Button
            android:id="@+id/btcheck"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="50dp"
            android:text="@string/check" />

        <TextView
            android:id="@+id/tvresult"
            android:layout_width="303dp"
            android:layout_height="91dp"
            android:layout_margin="50dp"
            android:fontFamily="@font/alike"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" />
    </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>